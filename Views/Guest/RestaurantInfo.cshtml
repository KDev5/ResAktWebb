@model IEnumerable<ResAktWebb.Models.RestaurantInfo>

@{
    ViewData["Title"] = "Index";
}
<div class="my-div">
    <h1>Öppetider</h1>


    @foreach (var item in Model)
    {
        <div>@ViewBag.OpenHours</div>
        <div>@Html.DisplayFor(modelItem => item.DayName): @Html.DisplayFor(modelItem => item.Open) - @Html.DisplayFor(modelItem => item.Closed)</div>
    }


</div>
